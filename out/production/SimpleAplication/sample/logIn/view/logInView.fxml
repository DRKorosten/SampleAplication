<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="MainPane" alignment="CENTER" hgap="10.0" prefHeight="400.0" prefWidth="600.0" stylesheets="/sample/logIn/view/style.css" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.logIn.controller.LogInController">
      <TextField fx:id="NameField" prefHeight="35.0" prefWidth="250.0" promptText="Name" GridPane.rowIndex="1">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="PassField" disable="true" prefHeight="35.0" prefWidth="250.0" promptText="Password" GridPane.rowIndex="3">
         <font>
            <Font size="17.0" />
         </font>
      </PasswordField>
      <Button fx:id="Sign" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#logInPressed" prefHeight="40.0" prefWidth="250.0" text="Log in" textAlignment="CENTER" GridPane.rowIndex="5">
         <font>
            <Font name="Calibri Light" size="21.0" />
         </font>
      </Button>
      <Button fx:id="Registration" alignment="CENTER" mnemonicParsing="false" prefHeight="43.0" prefWidth="215.0" text="Registration" translateX="200.0" translateY="-70.0">
         <font>
            <Font name="Cambria Math" size="21.0" />
         </font>
      </Button>
</GridPane>
